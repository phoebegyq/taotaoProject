	<script type="text/javascript">
		var dataLayer = null;

		//********过渡页切新版推荐-start***************
		function getParam(paramName) {
			var paramValue = "";
			isFound = false;
			if (this.location.search.indexOf("?") == 0
					&& this.location.search.indexOf("=") > 1) {
				arrSource = unescape(this.location.search).substring(1,
						this.location.search.length).split("&");
				i = 0;
				while (i < arrSource.length && !isFound) {
					if (arrSource[i].indexOf("=") > 0) {
						if (arrSource[i].split("=")[0].toLowerCase() == paramName
								.toLowerCase()) {
							paramValue = arrSource[i].split("=")[1];
							isFound = true;
						}
					}
					i++;
				}
			}
			return paramValue;
		}
		try {
			var nr = getParam("nr");
			var heads = document.getElementsByTagName("head");
			var link = document.createElement("link");
			var href = nr ? "//misc.360buyimg.com/user/cart/widget/??addtocart-201608/addtocart-201608.css"
					: "//misc.360buyimg.com/user/cart/widget/??addtocart-2016/addtocart-2016.css?v=201609201019";
			link.setAttribute("rel", "stylesheet");
			link.setAttribute("type", "text/css");
			link.setAttribute("href", href);
			heads[0].appendChild(link);
		} catch (e) {
		}
		//********过渡页切新版推荐-end***************
	</script>
